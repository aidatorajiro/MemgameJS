{
  "name": "memgame",
  "version": "2.0.0",
  "description": "A process memory adventure game.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack:osx": "rm -rf ./dist/Memgame-darwin-x64 && electron-packager . \"Memgame\" --platform=darwin --arch=x64 --version=1.7.9 --no-prune --out=\"dist\"",
    "pack:win": "rm -rf ./dist/Memgame-win32-ia32 && electron-packager . \"Memgame\" --platform=win32 --arch=ia32 --version=1.7.9 --no-prune --out=\"dist\"",
    "build:osx": "npm run pack:osx && zip -j \"dist/Memgame-darwin-x64.zip\" \"dist/Memgame-darwin-x64\"",
    "build:win": "npm run pack:win && zip -j \"dist/Memgame-win32-ia32.zip\" \"dist/Memgame-win32-ia32\""
  },
  "repository": "https://github.com/aidatorajiro/memgame",
  "keywords": [
    "Electron",
    "game",
    "memory",
    "adventure",
    "adventure game"
  ],
  "author": "Torajiro Aida",
  "license": "MIT",
  "devDependencies": {
    "electron": "1.7.9",
    "electron-packager": "^11.0.1"
  },
  "dependencies": {
    "ffi": "^2.2.0",
    "three": "^0.88.0"
  }
}
