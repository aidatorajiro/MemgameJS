{
  "name": "memgame",
  "version": "2.0.0",
  "description": "A process memory adventure game.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "lint": "standard",
    "pack:osx": "rm -rf ./dist/Memgame-darwin-x64 && electron-packager . \"Memgame\" --platform=darwin --arch=x64 --version=1.7.9 --no-prune --out=\"dist\"",
    "pack:win": "rm -rf ./dist/Memgame-win32-x64 && electron-packager . \"Memgame\" --platform=win32 --arch=x64 --version=1.7.9 --no-prune --out=\"dist\""
  },
  "repository": "https://github.com/aidatorajiro/memgame",
  "keywords": [
    "Electron",
    "game",
    "memory",
    "adventure",
    "adventure game"
  ],
  "author": "Torajiro Aida",
  "license": "MIT",
  "devDependencies": {
    "electron": "1.8.4",
    "electron-packager": "^11.0.1",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "ffi": "^2.2.0",
    "three": "^0.88.0"
  },
  "standard": {
    "globals": [
      "requestAnimationFrame"
    ]
  }
}
